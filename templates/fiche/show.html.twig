{% extends 'base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('fiche') }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('mapsBuilder') }}
{% endblock %}

{% block body %}
    <h1>{{ fiche.title }}</h1>

    <a href="{{ url('fiche.edit', {'categoryId': category.id, 'ficheId': fiche.id}) }}">Edit</a>

    <div id="fiche" style="width:600px;">
    {{ ficheView|raw }}
    </div>

{% endblock %}